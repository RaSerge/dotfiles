# Store here your os-local zsh settings
# This is for OS X

# Set the terminal type:
export TERM="xterm-256color"

# Aliases
# Open new MacVim buffers in the same MacVim window
alias gvim='open -a MacVim '

function vim () {
    if hash nvim 2>/dev/null; then
        nvim "$@"
    else
        vim "$@"
    fi
}
