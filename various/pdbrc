import sys
import os.path
import rlcompleter
import pdb
import pprint as pprint

# Exec file helper
# If ~/.pdbrc.py is missing, you get an error message (which doesn't hurt).
execfile(os.path.expanduser("~/.pdbrc.py"))

pdb.Pdb.complete = rlcompleter.Completer(locals()).complete
